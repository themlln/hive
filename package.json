{
  "name": "hive",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "engines": {
    "node": ">= 7.0.0"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "ts-node-dev src/index.ts",
    "test-db": "ts-node-dev src/db.ts",
    "test": "NODE_ENV='test' mocha \"./src/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" "
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@types/axios": "^0.14.0",
    "@types/fabric": "^3.6.1",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "axios": "^0.19.2",
    "class-validator": "^0.11.0",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^6.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.1",
    "fabric": "^4.0.0-beta.6",
    "history": "^4.9.0",
    "material-ui": "^0.20.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.7.2",
    "react-color": "^2.18.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.3.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typeorm": "^0.2.22",
    "typeorm-store": "^1.2.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.7",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.3",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/sequelize": "^4.28.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "axios-mock-adapter": "^1.16.0",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "git-url-parse": "^11.1.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.3",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "react": "^16.12.0",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "simple-git": "^1.121.0",
    "source-map-loader": "^0.2.4",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "yaml": "^1.6.0"
  }
}
